    "events": "^1.0.2",
    "express": "4.12.3",
    "express-session": "1.11.1",
    "flux": "^2.0.1",
    "i18n-light": "^0.2.1",
    "jsdoc": "3.3.2",
    "lodash": "3.10.0",
    "log4js": "^0.6.26",
    "markdown": "^0.5.0",
